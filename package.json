{
  "name": "api-toaster",
  "version": "0.0.1",
  "description": "ApiToaster",
  "productName": "ApiToaster",
  "author": "https://github.com/Virus288",
  "license": "Apache-2.0",
  "private": false,
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "/lib",
    "/types"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Virus288/ApiToaster"
  },
  "keywords": [
    "api",
    "express",
    "time travel",
    "test",
    "testing"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "lint": "cross-env NODE_ENV=development eslint -c eslint.config.mjs . --fix",
    "listErrors": "node_modules/.bin/tsc --skipLibCheck",
    "lintStaged": "npx lint-staged",
    "test": "echo 'If you want to run all the tests, please use makefile'",
    "test:e2e": "jest --runInBand --config __tests__/jest.config.e2e.ts",
    "test:unit": "jest --config __tests__/jest.config.unit.ts"
  },
  "lint-staged": {
    "*.ts": [
      "cross-env NODE_ENV=development eslint --cache"
    ],
    "*.json,.{eslintrc,prettierrc}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ]
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "chalk": "5.3.0",
    "winston": "3.14.2",
    "winston-daily-rotate-file": "5.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.0",
    "@jest/globals": "29.7.0",
    "@types/express": "^4.17.21",
    "@types/node": "22.5.2",
    "@typescript-eslint/eslint-plugin": "8.4.0",
    "@typescript-eslint/parser": "8.4.0",
    "cross-env": "7.0.3",
    "eslint": "9.9.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-check-file": "2.8.0",
    "eslint-plugin-import": "2.30.0",
    "eslint-plugin-jest": "28.8.2",
    "eslint-plugin-jsdoc": "^50.0.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-sort-class-members": "1.20.0",
    "express": "^4.19.2",
    "jest": "29.7.0",
    "jest-config": "29.7.0",
    "prettier": "3.3.3",
    "ts-jest": "29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.2",
    "typescript": "5.5.4"
  },
  "overrides": {
    "eslint-plugin-import": {
      "eslint": "^9"
    }
  }
}
